    name = Andy Gayton
    email = andy@thecablelounge.com

[core]
    quotepath = false

[push]
    default = simple

[branch "master"]
    rebase = true

[color]
    ui = 1

[color "branch"]
    current = yellow reverse
    local = yellow
    remote = green

[color "diff"]
    whitespace = red reverse

[color "status"]
    added = yellow
    changed = green
    untracked = cyan
